{
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "App\\Controllers\\": "app/controllers/",
      "App\\Core\\": "app/core/",
      "App\\Models\\": "app/models/",
      "App\\Services\\": "app/services/",
      "App\\Utilities\\": "app/utilities/",
      "App\\Interfaces\\": "app/interfaces/",
      "Config\\": "config/",
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  },
  "require": {
    "php": ">8.3",
    "vlucas/phpdotenv": "^5.6"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
